#include <stdio.h>

int main()
{
    int num1,num2,num3,num4,num5;
    
    printf("enter num1:");
    scanf("%d",&num1);
    printf("enter num2:");
    scanf("%d",&num2);
    printf("enter num3:");
    scanf("%d",&num3);
    printf("enter num4:");
    scanf("%d",&num4);
    printf("enter num5:");
    scanf("%d",&num5);
    
    printf("enter num1=%d\n",num1);
    printf("enter num2=%d\n",num2);
    printf("enter num3=%d\n",num3);
    printf("emter num4=%d\n",num4);
    printf("enter num5=%d\n",num5);
    
    
    int total=num1+num2+num3+num4+num5;
    printf("total=%d\n",total);
    
    float avarage=total/2.0;
    printf("avarage=%.1f",avarage);

    return 0;
}
